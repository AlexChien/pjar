<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
    <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title><%= t("site.title") %></title>
    <script language="javaScript" type="text/javascript" SRC="/javascripts/SWAPIMGRESTORE.JS"></SCRIPT>
    <link href="/stylesheets/style.css" rel="stylesheet" type="text/css">
    <script language="JavaScript" type="text/JavaScript">
    <!--
    function MM_openBrWindow(theURL,winName,features) { //v2.0
      window.open(theURL,winName,features);
    }
    //-->
    </script>
    <%= yield(:header) %>
    </head>

<body bgcolor="#646459" background="/images/bg_1.jpg" class="bg" leftmargin="0" topmargin="90" marginwidth="0" marginheight="0">
<table id="placeholder" width="100%" border="0" cellspacing="0" cellpadding="0">
      <tr>
    <td width="10" rowspan="3" valign="top"><img src="/images/spacer.gif" width="10" height="1"></td>
    <td width="220" rowspan="3" valign="top"><!--- 导航表格 -->

      <table id="lNav" width="100%" height="400" border="0" cellpadding="0" cellspacing="0" bgcolor="#E6E6DC">
        <tr>
          <td height="55" valign="bottom"><a href="/"><img src="/images/logo_cn.gif" alt="" border="0"></a><br></td>
        </tr>
        <tr>
          <td align="center" valign="top"><br>

            <!-- 导航开始  -->
            <table width="180" border="0" cellspacing="3" cellpadding="0">
              <%= render :partial => 'shared/nav' %>
            </table>
            <!-- 导航结束 --></td>
          </tr>
          <tr>
            <td bgcolor="#B3B39A" height="1">
              <img src="/images/spacer_g.gif" width="1" height="1">
            </td>
          </tr>
          <%= render :partial => 'shared/nav_secondary' %>
        </table>

          <!-- 导航表格结束 --></td>
    <td height="8" colspan="4" valign="top"><img src="/images/spacer.gif" width="1" height="1"></td>
  </tr>
      <tr>
    <!-- 地址栏 -->
    <td height="20" colspan="4" valign="bottom" bgcolor="#B3B39A" class="address">&nbsp;<a href="../index.htm" class="address">&nbsp;<%= t("site.bread.home") %></a> > <b><%= t("site.#{current_section}") %></b></td>
    <!-- 地址栏 -->
  </tr>
  <tr>
    <td width="3" valign="top"><table width="3" height="372" border="0" cellpadding="0" cellspacing="0">
      <tr>
        <td bgcolor="#B3B39A"><img id="lSep" src="/images/spacer.gif" width="3" height="1"></td>
      </tr>
    </table></td>
    <!-- 内容表格 -->
    <%= yield %>
    <!-- 内容表格结束 -->
    <td width="3" valign="top"><table width="3" height="372" border="0" cellpadding="0" cellspacing="0">
      <tr>
        <td bgcolor="#B3B39A"><img id="rSep" src="/images/spacer.gif" width="3" height="1"></td>
      </tr>
    </table></td>
    <td valign="top" bgcolor="#E6E6DC">&nbsp;</td>
  </tr>
    </table>
<img src="/images/copy.gif">
</body>
</html>
